{
  "name": "@yandex-cloud/nodejs-sdk",
  "version": "3.0.0",
  "description": "Yandex.Cloud NodeJS SDK",
  "keywords": [
    "yandex-cloud",
    "sdk",
    "cloud"
  ],
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/yandex-cloud/nodejs-sdk.git"
  },
  "author": "Yandex LLC",
  "license": "MIT",
  "main": "dist/index.js",
  "bugs": {
    "url": "https://github.com/yandex-cloud/nodejs-sdk/issues"
  },
  "files": [
    "dist"
  ],
  "workspaces": [
    "packages/*"
  ],
  "homepage": "https://github.com/yandex-cloud/nodejs-sdk#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^15.0.0",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/lodash": "^4.14.178",
    "@types/luxon": "^2.0.8",
    "@types/mustache": "^4.2.5",
    "@types/node": "^16.11.3",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "@yandex-cloud/core": "^3.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prefer-arrow-functions": "^3.2.4",
    "eslint-plugin-unicorn": "^49.0.0",
    "fast-glob": "^3.3.2",
    "grpc-tools": "^1.12.4",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lerna": "^8.0.0",
    "protobufjs": "^7.2.5",
    "mustache": "^4.2.0",
    "semantic-release": "^22.0.12",
    "ts-jest": "^29.1.1",
    "ts-proto": "^1.165.1",
    "tsx": "4.6.2",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "test:scripts": "jest -c config/jest-scripts.ts",
    "test": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" jest -c config/jest.ts --passWithNoTests '.*\\.test\\.ts$'",
    "lint": "eslint src config",
    "build": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" tsc -p .",
    "generate-code": "tsx scripts/generate-code/index.ts && lerna run build",
    "check-endpoints": "tsx scripts/check-endpoints.ts",
    "prepare": "husky install",
    "prepublishOnly": "npm run build && lerna run build && lerna run test && lerna run lint",
    "local-publish": "lerna publish --no-push --registry='http://localhost:4873/'",
    "local-unpublish-all": "lerna exec -- npm unpublish -f --registry='http://localhost:4873/' \"\\${LERNA_PACKAGE_NAME}\""
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "nx": {
    "targetDefaults": {
      "build": {
        "dependsOn": [
          "^build"
        ]
      }
    }
  }
}
