{
  "compilerOptions": {
    "lib": ["ES2022"],
    "module": "Node16",
    "target": "ES2022",
    "allowJs": true,
    "declaration": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
  },
  "include": [
    "./src"
  ]
}
